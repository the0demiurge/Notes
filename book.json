{
  "title": "CharlesNotes",
  "author": "Charles Xu",
  "plugins": [
    "katex",
    "copy-code-button",
    "graphviz@git+https://github.com/darvasd/gitbook-plugin-graphviz.git",
    "disqus",
    "expandable-chapters"
  ],
  "ignores": [
    "_book",
    "node_modules"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "notes-pythonic-life"
    }
  },
  "pdf": {
    "fontFamily": "SimSun"
  },
  "graphviz": {
    "format": "svg",
    "engine": "dot"
  }
}
